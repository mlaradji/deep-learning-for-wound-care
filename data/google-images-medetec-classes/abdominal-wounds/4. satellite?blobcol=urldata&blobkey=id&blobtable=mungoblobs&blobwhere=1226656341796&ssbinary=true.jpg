        XA.component.rbaForms = (function ($) {
            var api = {};

            api.init = function () {
                $(window).bind('load', function () {
                    var form = $('.acelity-captcha-form');
                    if (form) {
                        $(form).submit(function () {
                            var recaptchaResponse = grecaptcha.getResponse($(this).index(".acelity-captcha-form").toString());
