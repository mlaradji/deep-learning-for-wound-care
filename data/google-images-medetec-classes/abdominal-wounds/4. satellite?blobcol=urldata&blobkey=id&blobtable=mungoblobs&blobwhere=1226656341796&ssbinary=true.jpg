                                //document.getElementById('captchaError').innerHTML = "Please verify that you are not a robot.";
                                $(this).find(".captchaError").text('Please verify that you are not a robot.');
                                return false;
                            }
                            else {
                                //document.getElementById('captchaError').innerHTML = "";
                                $(this).find(".captchaError").text('');
                                $(this).find('.googleRecaptchaClass').val(recaptchaResponse);
                                return true;
                            }
                        });
                    }

                    var loginForm = $('.acelity-captcha-login-form').children('form')[0];
                    if (loginForm) {
                        $(loginForm).submit(function (e) {
                            var recaptchaResponse = grecaptcha.getResponse();
                            if (recaptchaResponse.length == 0) {
                                //document.getElementById('captchaError').innerHTML = "Please verify that you are not a robot.";
                                $(this).find(".captchaError").text('Please verify that you are not a robot.');
                                e.preventDefault();
                                return false;
                            }
                            else {
                                //document.getElementById('captchaError').innerHTML = "";
                                $(this).find(".captchaError").text('');
                                $(this).find('.googleRecaptchaClass').val(recaptchaResponse);
                                return true;
                            }
                        });
                    }
                });
            };
            return api;

        }(jQuery, document));
